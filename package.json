    "sales",
    "commission",
    "tracker"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "xlsx": "^0.18.5",
    "serverless-http": "^3.2.0"
  }
}
